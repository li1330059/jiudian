
page {
	  width: 100%;
	  height: 100%;
}
.mine{
	 width: 100%;
	 min-height: 100vh;
	 
	background: url("https://jhd.wangshangquan.top/static/img/bag.png");
	background-size: 100% 100%;
	background-repeat: no-repeat;
	background-attachment: fixed;
	/* background-color: #ffd102; */
	/* background-image: linear-gradient(to right, #f7e6d6, #d6b89c); */
	/* background-color: #f4f4f4; */
}
.mine button{
	padding: 0;
	line-height: auto;
	background-color: none;
	width: 130rpx;
	height: 130rpx;
	border-radius: 50%;
}
.mine .mHead{
	width: 100%;
	height: 320rpx;
	display: flex;
	align-items: center;

	/* background-image: linear-gradient(to right bottom, #8aedc9, #a5e1dd,#d5d6b0,#f9eeb6); */
}
.mine .mHead .mmhd{
	width: 90%;
	margin: 0 auto;
	display: flex;
	align-items: center;
}
.mine .hhb{
	width: 100%;
	height: 46rpx;
	line-height: 46rpx;
	margin-right: 37rpx;
	/* background-image: linear-gradient(to bottom, #eaecf0, #e7f0f2); */
	text-align: right;
	font-size: 20rpx;
	color:#777878;
}
.mine .mHead .toux{
	
	width: 130rpx;
	height: 130rpx;
	/* background-color: #e6eff9; */
	border-radius: 50%;
	margin-top: 80rpx;
}
.mine .mHead .namme{
/* 	margin-left: -80rpx;
	; */
}
.mine .mBody{
	width: 100%;
	
	/* background-color: #fff; */
	/* background-image: linear-gradient(to bottom,#e1e8e9, #c9e6ff, #d8e8f5); */
}
.mine .mBody .myOrder{
	font-size: 1rem;
	font-weight: bold;
	padding-top: 40rpx;
	padding-left: 40rpx;
}
.mine .mBody .iccon{
	margin: 10rpx auto 0;
	width: 95%;
	font-size: 28rpx;
	background-color: #fff;
	border-radius: 10rpx;
}
.mine .mBody .iccon ._ul{
	width: 90%;
	display: flex;
	justify-content: space-between;
	margin: 0 auto;
}
.mine .mBody .iccon ._ul ._li{
	width: 120rpx;
	text-align: center;
}
.mine .mBody .tiao{
	width: 100%;
	height: 10rpx;
	background-color: #cbe0ef;
	margin-top: 50rpx;
}
.mine .mBody .list{
	width: 100%;
	border-radius: 10rpx;
}
.mine .mBody .list ._ul{
	width: 85%;
	margin: 0 auto;
	display: block;
}
.mine .mBody .list ._ul ._li{
	height: 100rpx;
	line-height: 100rpx;
	width: 100%;
	font-size: 0.8rem;
	display: flex;
	justify-content: space-between;
}
.mine .mBody .dii{
	width: 100%;
	min-height: 20vh;
}
.mine  .member{
		width: 90%;
		margin: 0 auto;
		padding-top: 37rpx;
		
		/* border-bottom: 4px solid #dddddd; */
}
.mine  .member .memberTitle{
		font-weight: bold;
}
.mine  .member .memberBody{
		width: 90%;
		padding-bottom: 10rpx;
		margin: 0 auto;
		border-bottom: 2rpx #e6e6e6 solid;
}
.mine  .member .memberBody .memberItem{
		width: 100%;
		display: flex;
		justify-content: space-between;
		margin-top: 20rpx;
		font-size: 28rpx;
}
.qdd{
	padding: 13rpx;
	background-image: linear-gradient(to right, #efa10c, #fcbf4b);
	color: #fff;
	font-size: 20rpx;
	border-radius: 50rpx;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
} 

