<view class="{{['w-time-picker',(showTimePicker)?'show':'']}}"><view class="w-time-days"><scroll-view scroll-x="{{true}}"><view class="w-time-scroll"><block wx:for="{{dayList}}" wx:for-item="day" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toggleIndex',['$0',index],[[['dayList','',index]]]]]]]}}" class="w-time-day" style="{{'color:'+(tabIndex==index?theme:'#333')+';'+('border-color:'+(tabIndex==index?theme:'#ddd')+';')}}" bindtap="__e"><view class="w-time-week">{{day.week}}</view><view class="w-time-date">{{day.month+"/"+day.day}}</view></view></block></view></scroll-view></view></view>